    "check": "npm-run-all lint type-check test",
    "check:ci": "npm-run-all lint type-check test:ci",
    "clean": "rimraf build esnext styles types docs \"build-intermediate\" \"index.*\" \"./src/styles/polaris-tokens\" \"styles.{css,scss}\"",
